<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<HBox spacing="20.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ge.vakho.gxt.editor.controller.AboutController">
   <children>
      <ImageView fx:id="aboutPageImageView" fitHeight="64.0" fitWidth="64.0" pickOnBounds="true" preserveRatio="true" />
      <VBox alignment="CENTER_LEFT" spacing="10.0" HBox.hgrow="ALWAYS">
         <children>
            <Label text="GXT Editor" />
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="This is a GXT editor for GTA III." />
         </children>
         <HBox.margin>
            <Insets />
         </HBox.margin>
      </VBox>
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</HBox>
